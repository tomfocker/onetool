{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/types/**/*.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/renderer/src/*"]
    },
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
